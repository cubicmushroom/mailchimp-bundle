{
  "name": "cubicmushroom/mailchimp-bundle",
  "version": "0.0.0",
  "description": "Symfony bundle for integrating the  MailChimp API",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "prefer-stable" : true,
  "license": "MIT",
  "authors": [
    {
      "name": "Toby Griffiths",
      "email": "toby@cubicmushroom.co.uk"
    }
  ],
  "require": {
    "php": ">=5.5",
    "symfony/symfony": "2.7.*",
    "equire mailchimp/mailchimp": "~2.0"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.2@dev"
  },
  "autoload": {
    "psr-0": {
      "": "src/"
    },
    "psr-4": {
      "spec\\": "spec/"
    }
  }
}
